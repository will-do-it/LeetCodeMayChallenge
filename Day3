class Solution {
public:
    bool canConstruct(string ransomNote, string magazine) {
        for(int i=0;i<ransomNote.length();i++){
            bool flag=false;
            for(int j=0;j<magazine.length();j++){
                if(ransomNote.at(i)==magazine.at(j)){
                    magazine.at(j)=' ';
                    flag=true;
                    break;
                }
            }
            if(flag==false)
                return false;
            
        }
        return true;
    }
};
